{
    "name": "keycloakify-starter",
    "version": "0.0.0",
    "description": "Starter for Keycloakify 11",
    "repository": {
        "type": "git",
        "url": "git://github.com/codegouvfr/keycloakify-starter.git"
    },
    "type": "module",
    "scripts": {
        "postinstall": "keycloakify sync-extensions",
        "dev": "vite",
        "build": "tsc && vite build",
        "build-keycloak-theme": "pnpm build && keycloakify build",
        "start": "keycloakify start-keycloak",
        "storybook": "storybook dev -p 6006",
        "format": "prettier . --write",
        "flush-keycloak": "rm -rf ./dist_keycloak",
        "eject-page": "keycloakify eject-page"
    },
    "license": "MIT",
    "keywords": [],
    "dependencies": {
        "@tailwindcss/vite": "^4.1.8",
        "autoprefixer": "^10.4.19",
        "keycloakify": "^11.13.2",
        "postcss": "^8.4.39",
        "react": "^18.2.0",
        "react-dom": "^18.2.0",
        "tailwind-merge": "^3.3.0",
        "tailwindcss": "^4.1.8",
        "@keycloakify/email-native": "~260007.0.0"
    },
    "devDependencies": {
        "@eslint/js": "^9.15.0",
        "storybook": "^8.1.10",
        "@storybook/react": "^8.1.10",
        "@storybook/react-vite": "^8.1.10",
        "@types/react": "^18.2.43",
        "@types/react-dom": "^18.2.17",
        "@typescript-eslint/eslint-plugin": "^8.15.0",
        "@typescript-eslint/parser": "^8.15.0",
        "@vitejs/plugin-react": "^4.2.1",
        "eslint": "^9.15.0",
        "eslint-config-prettier": "^9.1.0",
        "eslint-plugin-react": "^7.37.2",
        "eslint-plugin-react-hooks": "^5.0.0",
        "eslint-plugin-react-refresh": "^0.4.14",
        "eslint-plugin-storybook": "^0.11.1",
        "globals": "^15.12.0",
        "prettier": "3.3.1",
        "storybook": "^8.1.10",
        "typescript": "^5.2.2",
        "typescript-eslint": "^8.15.0",
        "vite": "^5.0.8"
    },
    "engines": {
        "node": "^18.0.0 || >=20.0.0"
    }
}
